					$('#Username').text(data1.username);
					if(data1.myColor.length>10){
						$('#39afnmf').css('background-color',data1.myColor);
					}
					if(data1.guildinfo != 0) {
						$('#guildinfo').html(data1.guildinfo);
					}
					$('#accPoints').text(data1.games_left);
					$('#accLevel').text(data1.accLevel);
					$('#accType').html(data1.accType);
					$('#accLastLogin').text(timeConverter(data1.accLastLogin));
					
			} else {
					if($('#typeTournament').is(':checked')) {
						window.location.href = '/tournament';
					} else {
						window.location.href = '/';
					}
			}
		}, "json");
	});*/

	$jT = $('#loginAccount');
	$jTnickL = $('#loginNick');
	$jTpassL = $('#loginPassword');
	$jT.click(function() {
		$("label[for='loginNick']").hide();
		$jTnickL.parent('div.form-group').removeClass( "has-error has-warning has-feedback" );
		if ($jTnickL.val().length != 0) {
			$.post( "/account", { getNameAndHash: $jTnickL.val(),password: $jTpassL.val() }, function( data ) {
				if(data.status==1) {
					$('#outputGuest').hide();
					$('#outputLogged').show();
					$('#nick').val(data.username+'#'+data.hash);
					$('#nick').prop('disabled', true);
					if($('#typeTournament').is(':checked')) {
						window.location.href = '/tournament';
					} else {
					window.location.href = '/';
					}
					logged = 1;
					createMenu('Account'); 
					
				} else {
					$("label[for='loginNick']").show().text(data.message);
					$jTnickL.parent('div.form-group').addClass( "has-error has-feedback" );
					$jTnickL.focus();
					
				}
			}, "json");
		} else {
			$jTnickL.parent('div.form-group').addClass( "has-warning has-feedback" );
			$jTnickL.focus();
		}
	});
	$('#registerNow').click(function() {createMenu('Account'); 
		createMenu('Register'); 
	});
	$jTnickL.keyup(function(e){
			if(e.which === 13){
				$jT.click();
			}
	});
	$jTpassL.keyup(function(e){
			if(e.which === 13){
				$jT.click();
			}
	});
});
</script>
