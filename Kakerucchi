<script>
function buyPremium() {
	
	$.post( "/refresh", { buy_premium: '1' }, function( data1 ) {
		if(parseInt(data1) == 2) {
			window.location.href = '/';
		} else {
			alert('You need at least  2 points!');
		}
	});
	
}
$('document').ready(function(){			
	createMenu('Home');
	/*
