<script>
function timeCount(count) {
	var counter=setInterval(timer, 1000);
	function timer() {
		count=count-1;
		if (count <= 0) {
			clearInterval(counter);
			$('#joinTournamentBtn').click();
			return;
		 }
		$('#countDown').text(count);
	}
}
$('#joinTournamentBtn').click(function() {
			$('#joinTournamentBtn').html('Checking...');	
			$('#joinTournamentBtn').prop('disabled', true);
			
			var regionID = parseInt($('#region').val());
			
			$.post( "/searchTournament", { fetch: '1',region:regionID }, function( data1 ) {
				switch(data1.status) {
					case 0: 
						$('#joinTournamentBtn').prop('disabled', false);
						$("label[for='loginNick']").show(); 
						$("label[for='loginNick']").text('Please login to continue!'); 
						$("label[for='loginNick']").parent('div.form-group').addClass( "has-error has-feedback" ); 
						$('#outputGuest').show();
						$('#outputLogged').hide();
						logged = 0;
						createMenu('Login'); 
						$('#joinTournamentBtn').html('Join the Tournmanet!');	
						break;
					case 1: 
						$('#joinTournamentBtn').html('Join the Tournmanet!');
						setRegion(data1.r2);
						setShowBorders(0);
						if(connect_audio.paused){
							connect_audio.play();
								} else {
							connect_audio.pause();
						}
						$('#playBtnResume').show();
						setGameMode(data1.message,1); 
						$("#overlays").fadeOut(2000);
						setTimeout(function() { $('#joinTournamentBtn').prop('disabled', false); setNick(document.getElementById('nick').value); },2000);
						
						break;
					case 2: 
						timeCount(10);
						$('#joinTournamentBtn').html(data1.message+'No servers available. (Retry in <span id="countDown">10</span>s)');
						break;
					case 3: 
						timeCount(60);
						$('#joinTournamentBtn').html('Start: 08/01/15 6:00');
						break;
					case 4:  
						timeCount(15);
						$('#joinTournamentBtn').html('No servers available. (Retry in <span id="countDown">15</span>s)');
						break;
					default: $('#joinTournamentBtn').html('Join the Tournmanet!'); $('#joinTournamentBtn').prop('disabled', false);
					
				}
		}, "json");
	});
</script>
