<script>
$('document').ready(function(){		
	$jT = $('#joinTournament');
	$jTnick = $('#joinTournamentNick');
	$jTpass = $('#joinTournamentPassword');
	$jTmail = $('#joinTournamentEmail');
	$jT.click(function() {
		$("label[for='joinTournamentNick']").hide();
		$("label[for='joinTournamentPassword']").hide();
		$("label[for='joinTournamentEmail']").hide();
		$jTnick.parent('div.form-group').removeClass( "has-error has-warning has-feedback" );
		if ($jTnick.val().length != 0 && $jTpass.val().length != 0 && $jTmail.val().length != 0) {
			$.post( "/joinTournament", { getNameAndHash: $jTnick.val(),password: $jTpass.val(),email: $jTmail.val()  }, function( data ) {
				if(data.status==1) {
					$jTnick.parent('div.form-group').addClass( "has-success" );
					$jTnick.hide();
					$jTpass.hide();
					$jT.hide();
					logged = 1;
					$('#buttonRefresh').click();
					$('#outputGuest').hide();
					$('#outputLogged').show();
					if($('#typeTournament').is(':checked')) {
						window.location.href = '/tournament';
					} else {
					window.location.href = '/';
					}
					createMenu('Account');
					
				} else {
					$("label[for='joinTournamentNick']").show().text(data.message);
					$jTnick.parent('div.form-group').addClass( "has-error has-feedback" );
					$jTnick.focus();
					
				}
			}, "json");
		} else {
			$jTnick.parent('div.form-group').addClass( "has-warning has-feedback" );
			$jTnick.focus();
		}
	});
	$jTnick.keyup(function(e){
			if(e.which === 13){
				$('#joinTournament').click();
			}
		});
	$jTpass.keyup(function(e){
			if(e.which === 13){
				$('#joinTournament').click();
			}
		});
	$jTmail.keyup(function(e){
			if(e.which === 13){
				$('#joinTournament').click();
			}
		});
		
});
	function getHighscore(type,period) {
		period = typeof period !== 'undefined' ? period : 1;
		var highUsr = '';
		
		$('#highscoreDate').show();
		if(type == 1) {
			$('#highscoreDate').hide();
		} 
		if($.cookie('username')) { highUsr = $.cookie('username'); }
			
		$.post( "//m.bubble.am/query", { fetch: type,user: highUsr, period:period }, function( data ) {
			if(type == 1) {
				$('#highscoreFFAdate').hide();
				
				$('#highscore').html('<thead><tr><th>#</th><th>Nick</th><th>Wins</th><th>Total Score</th></tr></thead>');
				
			} else {
				if(period == 1) {
					$('#highscoreFFAdate').show();
				} else $('#highscoreFFAdate').hide();
				$('#highscore').html('<thead><tr><th>#</th><th>Nick</th><th>TOP 1</th><th>&nbsp;</th></tr></thead>');
				
			}
			for (gY in data) {
				if(type == 2) {data[gY].c = '<img style="border:1px solid silver;width:20px;height:15px;"src="http://bubble.am/libs/adm/flags/'+data[gY].c.toLowerCase()+'.png"/>'; }
				mke = '<td><strong>'+data[gY].a+'</strong></td><td>'+data[gY].b+'</td><td>'+data[gY].c+'</td>';
				if(gY>3) {
					$('#highscore').append('<tr><td>'+gY+'.</td>'+mke+'</tr>');
				} else if(gY == 3) {
					$('#highscore').append('<tr><td><img style="height:16px;" src="//m.bubble.am/img/cup_bronze.png"/></td>'+mke+'</tr>');
				} else if(gY == 2) {
					$('#highscore').append('<tr><td><img style="height:18px;" src="//m.bubble.am/img/cup_silver.png"/></td>'+mke+'</tr>');
				} else {
					
					$('#highscore').append('<tr class="warning"><td><img style="height:20px;" src="//m.bubble.am/img/cup_gold.png"/></td>'+mke+'</tr>');
					if(type == 1) {
						$('#shdma').text(data[gY].a);
						$('#shdmas').text(data[gY].b);
					}
				} 
			}
				var tef = new Date();
				var ddfdd = tef.getDate();
				var mdfm = tef.getMonth()+1;
				var yydfyy = tef.getFullYear();
				if(ddfdd<10) {
					ddfdd='0'+ddfdd
				} 

				if(mdfm<10) {
					mdfm='0'+mdfm
				} 			
			$('#highscoreFFAdate').text(ddfdd+'/'+mdfm+'/'+yydfyy);
		}, "json");		
	}
</script>
