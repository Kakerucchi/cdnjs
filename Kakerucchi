<script>

$(document).ready(function() {

	    $('input[type=radio][name=selectType]').change(function() {
        if (this.value == '1') {
            $('#joinTournamentBtn').hide();
			$('#tournamentFirst').hide();
			$('#normalFirst').show();
            $('#playBtn').show();
            $('#gamemode').show();
            $('#playBtnResume').hide();
        }
        else if (this.value == '2') {
			$('#tournamentFirst').show();
			$('#normalFirst').hide();
            $('#joinTournamentBtn').show();
            $('#playBtn').hide();
            $('#gamemode').hide();

        }
    });
});
</script>
